    <javascript id="d31b7d1b-ef63-4602-88fc-eb79de7aede0"><![CDATA[function(e) {
  let state = initData.state;

  if(state == "disposal") {
    dews.confirm('폐기시 데이터가 완전 삭제되어 복원할 수 없습니다.\n 계속 하시겠습니까?').yes(function() {
      doMove(state);
    }).no(function () {
      return false;
    });
  } else {
    doMove(state);
  }  
}]]></javascript>
