    <javascript id="e152e8cd-4423-439f-96b1-070070622426"><![CDATA[function(e) {
  // '복원'의 경우 수정가능
  if(initData.state == "restore") {
    return true;
  }
  // '폐기' 
  else {
    if(e.row.data.CTRL_CLAS_CD == '100') {
      return true;
    } else {
      // 기본정보 선택된 경우 수정불가능
      if("Y" == self.gd_selectedEmp.getCellValue(self.gd_selectedEmp.select(), 'CTRL_CLAS_CD_100')) {
        return false;
      } else {
        return true;
      }
    }
  }
}]]></javascript>
